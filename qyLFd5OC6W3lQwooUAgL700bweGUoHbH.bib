@article{giannini_national_2009,
  title = {National vs Local Funding for Education: Effects on Growth and Inequality},
  volume = {23},
  url = {http://www.informaworld.com/10.1080/02692170902811785},
  abstract = {This paper develops a two-period overlapping generations model with heterogeneous agents aiming at analysing how decentralization in the provision of public education affects growth and personal inequality via human capital investment. Education is financed by a tax levied by either national or local authorities. The tax rate is chosen according to a median voter mechanism. During their working period of life, individuals look after their offspring by providing them with a high level of school education stemming from taxation. In addition parent's contributions to the social security system provide them with retirement income. Heterogeneity accounts for the differences in the optimal taxation mechanism, linking the income distribution to the tax rate, and hence to human capital accumulation, growth and income inequality. In this way we relate differences among agents to the tax rate. We show that decentralization induces growth rate disparities among local communities but it can be ruled out by a proper fiscal substitution between social security and locally provided education. Unlike in the literature, this type of fiscal design allows local economies to grow faster and more equally than the national design.},
  timestamp = {2009-04-26T21:23:53Z},
  number = {3},
  journaltitle = {International Review of Applied Economics},
  author = {Giannini, Massimo},
  date = {2009},
  pages = {367 -- 385}
}


