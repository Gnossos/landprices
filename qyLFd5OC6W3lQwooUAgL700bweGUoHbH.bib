  keywords = {banking,financialization,heterodox economics,marxism,rentiers},
  file = {Full Text PDF:/Users/marsh/Library/Application Support/Firefox/Profiles/i4lk844n.default-1416966597019/zotero/storage/RKFQVUTS/Lapavitsas - 2011 - Theorizing financialization.pdf:application/pdf;Snapshot:/Users/marsh/Library/Application Support/Firefox/Profiles/i4lk844n.default-1416966597019/zotero/storage/DFVBFTD2/611.html:text/html}
